<h1>发布工作职位</h1>

<%= simple_form_for [:admin,@job] do |f| %>

<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_upper_bound, :label => "薪水上限" %>
<%= f.input :wage_lower_bound, :label => "薪水下限" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>

<%= f.submit "Submit" %>
<% end %>
